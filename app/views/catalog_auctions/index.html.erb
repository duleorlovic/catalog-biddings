<p id="notice"><%= notice %></p>

<h1>Listing Catalog Auctions</h1>

<table>
  <thead>
    <tr>
      <th>Catalog</th>
      <th>Initial price</th>
      <th>Is active</th>
      <th>Expires at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalog_auctions.each do |catalog_auction| %>
      <tr>
        <td><%= catalog_auction.catalog %></td>
        <td><%= catalog_auction.initial_price %></td>
        <td><%= catalog_auction.is_active %></td>
        <td><%= catalog_auction.expires_at %></td>
        <td><%= link_to 'Show', catalog_auction %></td>
        <td><%= link_to 'Edit', edit_catalog_auction_path(catalog_auction) %></td>
        <td><%= link_to 'Destroy', catalog_auction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catalog auction', new_catalog_auction_path %>
